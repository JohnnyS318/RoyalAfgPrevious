{
  "name": "RoyalAfg",
  "version": "1.0.0",
  "description": "A Casino Website for the Anne Frank Gymnasium",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnnyS318/RoyalAfg.git"
  },
  "author": "Jonas Schneider <jonas.max.schneider@gmail.com>",
  "license": "MIT",

  "scripts": {
    "start": "concurrently \"yarn run web\" \"yarn run server\"",
    "start:dev": "concurrently \"yarn run web\" \"yarn run server:dev\"",
    "web": "cd ./web && ng serve",
    "server:dev": "cd ./server && yarn run start:dev",
    "server": "cd ./server && yarn run start"
  },

  "dependencies": {
    "concurrently": "^5.1.0"
  }
}
